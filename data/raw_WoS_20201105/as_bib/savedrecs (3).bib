@article{ ISI:000305035600020,
Author = {Duennes, Michelle A. and Lozier, Jeffrey D. and Hines, Heather M. and
   Cameron, Sydney A.},
Title = {{Geographical patterns of genetic divergence in the widespread
   Mesoamerican bumble bee Bombus ephippiatus (Hymenoptera: Apidae)}},

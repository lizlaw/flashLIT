@article{ ISI:000295310900003,
Author = {Lo Verde, Gabriella and La Mantia, Tommaso},
Title = {{The role of native flower visitors in pollinating Opuntia ficus-indica
   (L.) Mill., naturalized in Sicily}},
Journal = {{ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY}},

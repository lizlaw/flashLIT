@article{ ISI:000304339100046,
Author = {Mudri-Stojnic, Sonja and Andric, Andrijana and Jozan, Z. and Vujic, A.},
Title = {{POLLINATOR DIVERSITY (HYMENOPTERA AND DIPTERA) IN SEMI-NATURAL HABITATS
   IN SERBIA DURING SUMMER}},
Journal = {{ARCHIVES OF BIOLOGICAL SCIENCES}},
Year = {{2012}},
Volume = {{64}},
Number = {{2}},
Pages = {{777-786}},
Abstract = {{The aim of this study was to assess species diversity and population
   abundance of the two main orders of pollinating insects, Hymenoptera and
   Diptera. The survey was conducted in 16 grassland fragments within
   agro-ecosystems in Vojvodina, as well as in surrounding fields with
   mass-flowering crops. Pollinators were identified and the Shannon-Wiener
   Diversity Index was used to measure their diversity. Five families, 7
   subfamilies, 26 genera and 63 species of insects were recorded. All four
   big pollinator groups investigated were recorded; hoverflies were the
   most abundant with 32\% of the total number of individuals, followed by
   wild bees -29\%, honeybees -23\% and bumblebees with 16\%.}},

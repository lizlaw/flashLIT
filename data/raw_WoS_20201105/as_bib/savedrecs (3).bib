@article{ ISI:000324527300094,
Author = {Klatt, Bjoern K. and Burmeister, Carina and Westphal, Catrin and
   Tscharntke, Teja and von Fragstein, Maximillian},
Title = {{Flower Volatiles, Crop Varieties and Bee Responses}},

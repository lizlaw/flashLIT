@article{ ISI:000322255500015,
Author = {Yin, Xue-Wei and Iovinella, Immacolata and Marangoni, Roberto and
   Cattonaro, Federica and Flamini, Guido and Sagona, Simona and Zhang,
   Long and Pelosi, Paolo and Felicioli, Antonio},
Title = {{Odorant-binding proteins and olfactory coding in the solitary bee Osmia
   cornuta}},
Journal = {{CELLULAR AND MOLECULAR LIFE SCIENCES}},
Year = {{2013}},
Volume = {{70}},
Number = {{16}},
Pages = {{3029-3039}},
Month = {{AUG}},
Abstract = {{Solitary bees are major pollinators but their chemical communication
   system has been poorly studied. We investigated olfactory coding in
   Osmia cornuta from two perspectives, chemical and biochemical. We
   identified (E)-geranyl acetone and 2-hexyl-1,3-dioxolane, specifically
   secreted by females and males, respectively. A transcriptome analysis of
   antennae revealed 48 ORs (olfactory receptors), six OBPs
   (odorant-binding proteins), five CSPs (chemosensory proteins), and a
   single SNMP (sensory neuron membrane protein). The numbers of ORs and
   OBPs are much lower than in the honeybee, in particular, C-minus OBPs
   are lacking in the antennae of O. cornuta. We have expressed all six
   OBPs of O. cornuta and studied their binding specificities. The best
   ligands are common terpene plant odorants and both volatiles produced by
   the bee and identified in this work.}},

@article{ ISI:000301030800002,
Author = {Mesquita, T. M. S. and Augusto, S. C.},
Title = {{Diversity of trap-nesting bees and their natural enemies in the
   Brazilian savanna}},
Journal = {{TROPICAL ZOOLOGY}},
Year = {{2011}},
Volume = {{24}},
Number = {{2}},
Pages = {{127-144}},
Month = {{DEC}},
Abstract = {{The objective of this study was to examine, the diversity of solitary
   bees nesting in pre-existing cavities (trap-nests) and their natural
   enemies in a neotropical savanna. The study was carried out in two
   savanna areas in the Triangulo Mineiro, Brazil, from February 2007 to
   September 2008. In both areas, trap-nests were introduced and inspected
   biweekly. Two hundred and seventy-eight nests were founded by eleven bee
   species. Centris analis (Fabricius 1804), Centris tarsata Smith 1874 and
   Tetrapedia curvitarsis Friese 1899 had the highest numbers of nests.
   Eight species of natural enemies were recorded, the most abundant being
   Physocephala bipunctata Macquart 1843 and Codioxys sp. Information about
   the most abundant ground-nesting species and the trap-nest types used to
   attract them will be useful to maintain these bees in pre-established
   areas and to conserve them for applied pollination in neotropical
   savannas.}},

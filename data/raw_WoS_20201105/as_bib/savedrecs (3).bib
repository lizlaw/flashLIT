@article{ ISI:000310341900007,
Author = {Goulson, D. and Rotheray, E. L.},
Title = {{Population dynamics of the invasive weed Lupinus arboreus in Tasmania,
   and interactions with two non-native pollinators}},
Journal = {{WEED RESEARCH}},

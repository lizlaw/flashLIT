@article{ ISI:000298346700021,
Author = {Wang, Ying and Kocher, Sarah D. and Linksvayer, Timothy A. and
   Grozinger, Christina M. and Page, Jr., Robert E. and Amdam, Gro V.},
Title = {{Regulation of behaviorally associated gene networks in worker honey bee
   ovaries}},
Journal = {{JOURNAL OF EXPERIMENTAL BIOLOGY}},

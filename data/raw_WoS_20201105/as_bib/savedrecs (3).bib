@article{ ISI:000277241100014,
Author = {Solmaz, Ilknur and Sari, Nebahat and Aka-Kacar, Yildiz and Yalcin-Mendi,
   N. Yesim},
Title = {{The genetic characterization of Turkish watermelon (Citrullus lanatus)
   accessions using RAPD markers}},

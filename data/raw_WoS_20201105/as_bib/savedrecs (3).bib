@article{ ISI:000270860100014,
Author = {Franco, Emanuella L. and Aguiar, Candida M. L. and Ferreira, Vinina S.
   and de Oliveira-Reboucas, Patricia L.},
Title = {{Plant Use and Niche Overlap Between the Introduced Honey Bee (Apis
   mellifera) and the Native Bumblebee (Bombus atratus) (Hymenoptera:
   Apidae) in an Area of Tropical Mountain Vegetation in Northeastern
   Brazil}},
Journal = {{SOCIOBIOLOGY}},
Year = {{2009}},
Volume = {{53}},
Number = {{1}},
Pages = {{141-150}},
Abstract = {{The present study analyzed plant use by Apis mellifera and by a native
   bumblebee (Bombus atratus) as well as their niche breadth and niche
   overlap by collecting foragers on flowers. Both bee species used a
   relatively wide spectrum of floral resources, but concentrated their
   foraging on just a few plants and substituted their principal sources
   over time; both had wide trophic niches (A. mellifera, H'=2.66 and B.
   atratus, H'=2.38). The total niche overlap was 0.27 although there were
   large variation among the different months (0.09 to 0.50), demonstrating
   that there were periods when niche overlap intensified between the
   introduced honey bees and the native bumblebees. Data on plant phenology
   in the region of the Chapada Diamantina Mountain Range indicated that
   our sampling occurred principally during the months of generally greater
   abundance of floral resources for insect visitors, and that this may
   have been one of the factors that contributed to the low overlap levels
   encountered during most months. Sampling to cover the months and years
   with low availability of floral resources would complement our
   understanding of the effects of intensification of niche overlap and
   could better help in evaluating the effects of the presence of Apis
   mellifera on the foraging of B. atratus in this habitat.}},

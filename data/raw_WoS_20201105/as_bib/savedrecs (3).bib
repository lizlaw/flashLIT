@article{ ISI:000267832600009,
Author = {Freitas, Breno M. and Imperatriz-Fonseca, Vera Lucia and Medina, Luis M.
   and Peixoto Kleinert, Astrid de Matos and Galetto, Leonardo and
   Nates-Parra, Guiomar and Quezada-Euan, J. Javier G.},
Title = {{Diversity, threats and conservation of native bees in the Neotropics}},
Journal = {{APIDOLOGIE}},
Year = {{2009}},
Volume = {{40}},
Number = {{3}},
Pages = {{332-346}},
Month = {{MAY-JUN}},
Abstract = {{The Neotropics bee fauna is very rich with 5000 recognised species,
   including 33 genera (391 species) of Meliponini, but it is estimated to
   be at least three fold greater in species richness. Deforestation,
   agriculture intensification and introduction/spread of exotic competing
   bee species are considered the main threats to most indigenous species,
   although other less obvious causes can affect the populations of some
   bee species locally. Efforts to conserve the native bee fauna include
   better knowledge of bee richness and diversity (standardized surveys,
   larger bee collections and appropriate identification of bee species)
   and of their population dynamics, raising of public and policy makers'
   awareness, commercial applications of bee products and services such as
   pollination and preservation of natural habitat.}},

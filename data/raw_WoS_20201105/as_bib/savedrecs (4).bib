@article{ ISI:000247121200012,
Author = {Benachour, Karima and Louadi, Kamel and Terzo, Michael},
Title = {{Role of wild and honey bees (Hymenoptera : Apoidea) in the pollination
   of Vicia faba L. var. major (Fabaceae) in Constantine area (Algeria)}},
Journal = {{ANNALES DE LA SOCIETE ENTOMOLOGIQUE DE FRANCE}},

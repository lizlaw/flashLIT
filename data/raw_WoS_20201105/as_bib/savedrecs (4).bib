@article{ ISI:000244385100012,
Author = {Larsson, Magnus and Franzen, Markus},
Title = {{Critical resource levels of pollen for the declining bee Andrena
   hattorfiana (Hymenoptera, Andrenidae)}},
Journal = {{BIOLOGICAL CONSERVATION}},

@article{ ISI:000258639600025,
Author = {d'Avila, Marcia and Marchini, Luis Carlos},
Title = {{Faunistic analysis of flower-visiting hymenoptera in cerradao fragment
   in Itirapina, SP}},
Journal = {{CIENCIA FLORESTAL}},
